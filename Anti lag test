local function DeltaAntiLag()
    local lighting = game:GetService("Lighting")
    local physics = game:GetService("PhysicsService")
    
    lighting.GlobalShadows = false
    physics.PhysicsEnvironmentalThrottle = 0.7
    
    if game:GetService("RunService"):IsEngineFeature("Delta") then
        pcall(function()
            lighting:SetAttribute("Delta_ShadowsQuality", 0)
            lighting:SetAttribute("Delta_LODQuality", 1)
        end)
    end
    
    task.spawn(function()
        for i, obj in ipairs(workspace:GetDescendants()) do
            if obj:IsA("BasePart") then
                obj.CanCollide = false
                obj.CanQuery = false
                if i % 250 == 0 then task.wait() end
            elseif obj:IsA("ParticleEmitter") then
                obj.LightEmission = 0
                obj.LightInfluence = 0
            end
        end
    end)
end

game.Loaded:Wait()
task.delay(2, DeltaAntiLag)
